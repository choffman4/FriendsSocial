{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "https://choffman.com/",
    "Audience": "https://choffman.com/",
    "Key": "This is a sample secret key - please don't use in production environment."
  },
  "GrpcService": {
    "Host": "grpc-mongo-posting-service",
    "Port": "80"
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "posting-service-api"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka-server:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    },
    "instance": {
      "hostName": "posting_service_api"
    }
  }
}
